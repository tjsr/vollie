{
  "name": "vollie",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "db:generate": "drizzle-kit generate",
    "db:migrate": "npm run db:generate && drizzle-kit migrate",
    "db:export:dev": "mkdir data && npx wrangler d1 export vollie-dev --local --no-schema --output=./data/export.sql",
    "db:export:dev-all": "npx wrangler d1 export vollie-dev --local --output=./data/export.sql",
    "db:init": "wrangler d1 create vollie-dev",
    "db:init:old": "npx wrangler d1 execute vollie-dev --local --file=./src/sql/schema.sql",
    "db:init:old:remote": "npx wrangler d1 execute vollie-prod --remote --file=./src/sql/schema.sql",
    "dev": "wrangler pages dev",
    "dev:workers": "wrangler dev",
    "build": "tsc && vite build --watch",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "npm run build && wrangler pages dev ./dist",
    "deploy": "npm run build && wrangler pages deploy ./dist",
    "test": "vitest",
    "cf-typegen": "wrangler types"
  },
  "dependencies": {
    "@emotion/react": "^11.13.3",
    "@emotion/styled": "^11.13.0",
    "@mui/material": "^6.1.1",
    "@rjsf/core": "^5.21.1",
    "@rjsf/mui": "^5.21.1",
    "@rjsf/utils": "^5.21.1",
    "@rjsf/validator-ajv8": "^5.21.1",
    "@tanstack/react-query": "^5.56.2",
    "drizzle-orm": "^0.33.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.26.2",
    "zustand": "^5.0.2"
  },
  "devDependencies": {
    "@cloudflare/vitest-pool-workers": "^0.5.12",
    "@cloudflare/workers-types": "^4.20240925.0",
    "@types/react": "^18.3.10",
    "@types/react-dom": "^18.2.22",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "@vitejs/plugin-react": "^4.2.1",
    "drizzle-kit": "^0.24.2",
    "eslint": "^8.57.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.6",
    "typescript": "^5.7.2",
    "vite": "^5.4.8",
    "wrangler": "^3.99.0"
  }
}
